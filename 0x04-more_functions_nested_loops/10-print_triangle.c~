#include "holberton.h"
/**
* print_square - prints #
*@size: number of times to print
*
*/
void print_square(int size)
{
	int y;
	int j;

	if (size > 0)
	{
		for (y = 1; y <= size; y++)
		{
			for (j = 1; j <= size; j++)
			{
				_putchar('#');
			}
			_putchar('\n');
		}
	}
	else
		_putchar('\n');
}
